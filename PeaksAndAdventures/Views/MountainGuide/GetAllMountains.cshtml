@model IEnumerable<AllMountainsViewModel>

@{
	ViewBag.Title = "Всички планини познати на планинския водач";
}

<h3 class="text-center">@ViewBag.Title</h3>
<hr />

<link rel="stylesheet" href="~/css/Mountain/allStyle.css" />

@if (Model.Any())
{
	@await Html.PartialAsync("_AllMountainsPartial")
}
else
{
	<h4 class="text-danger text-lg-center">Няма записани планини!</h4>
}
